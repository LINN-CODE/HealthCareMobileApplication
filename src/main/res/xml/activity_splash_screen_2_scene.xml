<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:duration="5000"
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@layout/activity_splash_screen_main"
        motion:constraintSetStart="@layout/activity_splash_screen_splash">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@+id/icon"
                motion:percentX="0"
                motion:percentY="0" />
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@id/splash_bottom"
                motion:percentX="0"
                motion:percentY="0" />
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="deltaRelative"
                motion:motionTarget="@id/splash_head"
                motion:percentX="0"
                motion:percentY="0" />
        </KeyFrameSet>
    </Transition>
</MotionScene>