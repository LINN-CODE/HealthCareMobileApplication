<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/user_mainPage_rowBackground"
        android:layout_width="379dp"
        android:layout_height="120dp"
        android:background="@drawable/rounded_rectangle"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.028"
        tools:srcCompat="@drawable/rounded_rectangle" />

    <TextView
        android:id="@+id/user_register_clinic_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="112dp"
        android:text="Clinic 1"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintEnd_toEndOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintTop_toTopOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/user_register_clinic_phone_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Phone Number"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintEnd_toEndOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintTop_toBottomOf="@+id/user_register_clinic_name"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/user_register_clinic_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintEnd_toEndOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintHorizontal_bias="0.447"
        app:layout_constraintStart_toStartOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintTop_toBottomOf="@+id/user_register_clinic_phone_number"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/user_register_clinic_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email Address"
        android:textColor="#FFFFFF"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintEnd_toEndOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintHorizontal_bias="0.441"
        app:layout_constraintStart_toStartOf="@+id/user_mainPage_rowBackground"
        app:layout_constraintTop_toBottomOf="@+id/user_register_clinic_address"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>